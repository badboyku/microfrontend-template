{
  "name": "microfrontend-template",
  "description": "This is a template for creating a micro-frontend react app.",
  "version": "1.0.18",
  "author": "badboyku",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/badboyku/microfrontend-template.git"
  },
  "main": "src/index",
  "engines": {
    "node": ">=18",
    "npm": ">=9"
  },
  "scripts": {
    "prepare": "husky install",
    "start": "NODE_ENV=development runtime-env-cra --config-name=./public/runtime-env.js && webpack-cli serve",
    "build": "webpack --mode production",
    "clean": "rimraf ./dist",
    "clean:modules": "rimraf ./node_modules",
    "clean:coverage": "rimraf ./test_coverage",
    "lint": "eslint \"./src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint \"./src/**/*.{js,jsx,ts,tsx}\" --fix",
    "lint-staged": "lint-staged",
    "prettier": "prettier --check \"./src/**/*.{css,scss,json}\"",
    "prettier:fix": "prettier --write \"./src/**/*.{css,scss,json}\"",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "lint-staged": {
    "src/**/*.{css,scss,json}": [
      "prettier --write"
    ],
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "usage",
          "corejs": "3.31.0"
        }
      ],
      [
        "@babel/preset-react",
        {
          "runtime": "automatic"
        }
      ],
      "@babel/preset-typescript"
    ]
  },
  "jest": {
    "globals": {
      "__RUNTIME_CONFIG__": {
        "NODE_ENV": "test",
        "REACT_APP_MY_VAR": ""
      },
      "IS_DEV": false,
      "IS_PROD": true
    },
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/index.tsx",
      "!src/bootstrap.tsx"
    ],
    "coverageDirectory": "test_coverage",
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    },
    "resetMocks": true,
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.[jt]sx?$": "babel-jest",
      "^.+\\.css$": "<rootDir>/jest.cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|cjs|ts|tsx|css|json)$)": "<rootDir>/jest.fileTransform.js"
    },
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.22.5",
    "@babel/runtime-corejs3": "^7.22.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.13.0",
    "runtime-env-cra": "^0.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/plugin-transform-runtime": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@svgr/webpack": "^8.0.1",
    "@testing-library/dom": "^9.3.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/react": "^18.2.12",
    "@types/react-dom": "^18.2.5",
    "babel-jest": "^29.5.0",
    "babel-loader": "^9.1.2",
    "browserslist": "^4.21.9",
    "bundle-loader": "^0.5.6",
    "camelcase": "^6.3.0",
    "core-js": "^3.31.0",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "dotenv": "^16.3.0",
    "eslint-config-badboyku": "^1.0.25",
    "html-webpack-plugin": "^5.5.3",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-watch-typeahead": "^2.2.2",
    "lint-staged": "^13.2.2",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss": "^8.4.24",
    "postcss-loader": "^7.3.3",
    "postcss-preset-env": "^8.5.0",
    "resolve-url-loader": "^5.0.0",
    "rimraf": "^5.0.1",
    "sass": "^1.63.4",
    "sass-loader": "^13.3.2",
    "typescript": "5.0.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.87.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.9.0"
  }
}
